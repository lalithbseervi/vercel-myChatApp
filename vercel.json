{
    "version": 2,
    "builds": [{
        "src": "myChatApp/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build"
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "myChatApp/wsgi.py"
        }
    ]
}
